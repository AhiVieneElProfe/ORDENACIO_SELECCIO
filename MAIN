#include <iostream>
#include <stdio.h>
#include <time.h>
#include <stdlib.h>


//int main(){
int Ordenamiento(){
	
	int vector [5]= {4 , 9 , 12 , 11 , 7};
	int i, menor, temp, j;
	for(i=0; i<5; i++){
		printf("%d\n", vector[i]);
	}
	
	
	for(i=0; i<4; i++){
		menor = i;
		for(j= i+1; j<5; j++){
			if(vector[j] < vector[menor]){
				menor = j;
			}
			
		}
		if(i!=menor){
			temp = vector[i];
		    vector[i] = vector[menor];
		    vector[menor] = temp; 
		}

	}
	
	for(i=0; i<5; i++){
		printf("\n%d\n", vector[i]);
	}
	
}
